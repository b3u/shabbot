{
  "name": "shabbot",
  "version": "1.0.0",
  "private": true,
  "description": "A Probot app",
  "author": "Binyamin Aron Green <binyamingreen@protonmail.com>",
  "license": "MIT",
  "repository": "https://github.com/binyamin/shabbot.git",
  "homepage": "https://github.com/binyamin/shabbot",
  "bugs": "https://github.com/binyamin/shabbot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "node-fetch": "^3.1.1",
    "probot": "^11.2.0"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "nock": "^13.0.11",
    "nodemon": "^2.0.7",
    "smee-client": "^1.2.2",
    "standard": "^16.0.3"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
